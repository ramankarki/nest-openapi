{
  "name": "nest-openapi",
  "author": "Raman Karki",
  "version": "1.0.6",
  "description": "Auto generate openapi spec from nest.js codebase with single command",
  "packageManager": "pnpm@9.15.0",
  "keywords": [
    "openapi",
    "nest.js",
    "auto generate"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=22.0.0"
  },
  "main": "./dist/main.js",
  "repository": "https://github.com/ramankarki/nest-openapi",
  "bin": {
    "nest-openapi": "./dist/main.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "redocly": "redocly",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "chokidar": "^4.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "class-validator-jsonschema": "^5.0.2",
    "commander": "^14.0.0",
    "esbuild-register": "^3.6.0",
    "glob": "^11.0.2",
    "json-schema-faker": "^0.5.9",
    "openapi-types": "^12.1.3",
    "reflect-metadata": "^0.2.2",
    "ts-json-schema-generator": "^2.4.0",
    "ts-morph": "^26.0.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "prettier": "^3.5.3"
  },
  "peerDependencies": {
    "@redocly/cli": "^1.34.3"
  },
  "files": [
    "dist/",
    "README.md",
    "tsconfig.json"
  ]
}
